{
    "version": 2,
    "name": "skillgap-ai-frontend",
    "builds": [{
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist"
        }
    }],
    "routes": [{
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "VITE_API_BASE_URL": "@api_base_url"
    },
    "build": {
        "env": {
            "VITE_API_BASE_URL": "@api_base_url"
        }
    }
}